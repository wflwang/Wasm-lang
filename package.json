{
  "name": "Wasm-lang",
  "displayName": "Wasm-lang",
  "description": "W-M assembly language support",
  "author":"MaxwellWang",
  "version": "0.0.1",
  "icon": "./icon/wasm-lang1.png",
  "private":"true",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "repository": {  
    "type": "git",  
    "url": "https://github.com/wflwang/Wasm-lang"  
  },  
  "contributes": {
    "languages": [{
      "id": "wasm",
      "aliases": ["wasm", "wasm"],
      "extensions": [".asm",".s",".inc",".ash",".a",".wasm",".masm",".dctasm",".dctinc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wasm",
      "scopeName": "source.wasm",
      "path": "./syntaxes/wasm.tmLanguage.json"
    }],
    "themes": [
      {
        "label": "wasm-lang-theme",
        "uiTheme": "vs-dark",
        "path": "./themes/wasmh-lang-theme-color-theme.json"
      }
    ]
  }
}
